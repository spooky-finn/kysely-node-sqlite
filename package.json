{
  "name": "kysely-node-sqlite",
  "description": "Kysely SQLite adapter for the built in node:sqlite module",
  "author": "Spooky Finn",
  "version": "1.1.0",
  "type": "module",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/spooky-finn/kysely-node-sqlite"
  },
  "homepage": "https://github.com/spooky-finn/kysely-node-sqlite",
  "files": [
    "dist/",
    "package.json",
    "README.md"
  ],
  "keywords": [
    "kysely",
    "sqlite",
    "node",
    "adapter",
    "database",
    "sql",
    "query",
    "builder",
    "query-builder",
    "querybuilder",
    "orm",
    "typescript",
    "database",
    "node:sqlite"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=22.5.0"
  },
  "scripts": {
    "build": "tsup",
    "test": "tsx --test \"src/{,**/}*.test.ts\""
  },
  "devDependencies": {
    "@swc/core": "^1.10.7",
    "@types/node": "^24.0.15",
    "prettier": "^3.4.2",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "kysely": "^0.28.3",
    "async-mutex": "^0.5.0"
  }
}
